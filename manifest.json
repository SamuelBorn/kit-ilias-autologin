{
  "manifest_version": 2,
  "name": "kit-ilias-autologin",
  "version": "1.0",

  "description": "Clicks through the ilias login page for you.",

  "content_scripts": [
    {
      "matches": ["https://ilias.studium.kit.edu/login.php*", "https://idp.scc.kit.edu/idp/profile/SAML2/Redirect/SSO*"],
      "js": ["login.js"]
    }
  ]
}

